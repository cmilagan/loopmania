<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<!-- This screen names your game file and selects the mode you are playing -->
<!-- vertically stacked label and button -->

<VBox alignment="CENTER" prefHeight="437.0" prefWidth="617.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="YOU WON!" />
      <!-- Button to switch back to the main menu -->
      <Button fx:id="backToMain" onAction="#switchToMainMenu" text="Return to Main Menu" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
